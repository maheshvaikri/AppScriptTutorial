/**
* Prints out Hello World.
*
* @customfunction
*/

function HELLOWORLD() {
  
  var msg = "Hello, World!";
  
  return msg;

}

/**
* Raises a base to a power.
*
* @param {2} num1    base
* @param {3} num2    exponent
* @returns {Number}  base raised to exponent
* @customfunction
*/

function BASEPOWER(num1, num2) {
  
  var result = Math.pow(num1, num2);
  
  return result;

}
